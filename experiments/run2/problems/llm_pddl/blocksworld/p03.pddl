(define (problem block-problem)
  (:domain blockworld)
  (:objects b1 b2 b3 b4 - block arm1 - arm)
  (:init (on b3 b2)
         (on b1 b3)
         (clear b4)
         (clear b2)
         (clear b1)
         (not (holding arm1 b1))
         (not (holding arm1 b2))
         (not (holding arm1 b3))
         (not (holding arm1 b4)))
  (:goal (and (on b2 b1) (on b3 b4))))