(define (domain blockworld)
  (:requirements :strips :equality)
  (:types block arm)
  (:predicates (on ?b1 - block ?b2 - block)
               (clear ?b - block)
               (holding ?a - arm ?b - block))
  (:functions (total-cost))
  (:action pickup
    :parameters (?a - arm ?b - block)
    :precondition (and (clear ?b) (not (holding ?a ?_)))
    :effect (and (holding ?a ?b)
                  (not (clear ?b))
                  (not (holding ?a ?_))
                  (increase (total-cost) 1)))
  (:action putdown
    :parameters (?a - arm ?b - block)
    :precondition (holding ?a ?b)
    :effect (and (clear ?b)
                  (not (holding ?a ?b))
                  (not (holding ?a ?_))
                  (increase (total-cost) 1)))
  (:action stack
    :parameters (?a - arm ?b1 ?b2 - block)
    :precondition (and (holding ?a ?b1) (clear ?b2))
    :effect (and (on ?b1 ?b2)
                  (not (holding ?a ?b1))
                  (not (clear ?b2))
                  (increase (total-cost) 1)))
  (:action unstack
    :parameters (?a - arm ?b1 ?b2 - block)
    :precondition (and (on ?b1 ?b2) (clear ?a))
    :effect (and (holding ?a ?b1)
                  (clear ?b2)
                  (not (on ?b1 ?b2))
                  (increase (total-cost) 1))))