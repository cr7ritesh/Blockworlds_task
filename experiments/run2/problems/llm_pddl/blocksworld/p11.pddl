(define (problem blocks-problem)
  (:domain block-world)
  (:objects b1 b2 b3 b4 b5 b6 b7 b8 - block
            arm1 - arm)
  (:init (clear b8)
          (on b7 b5)
          (on b4 b2)
          (on b8 b3)
          (on b3 b7)
          (on b2 b1)
          (on b6 b4)
          (on b5 b6)
          (on b1 table)
          (holding arm1 none))
  (:goal (and (on b1 b8)
              (on b2 b3)
              (on b3 b5)
              (on b4 b2)
              (on b8 b7)))
)