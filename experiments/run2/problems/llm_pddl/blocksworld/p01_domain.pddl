(define (domain blockworld)
  (:requirements :strips :equality)
  (:types block arm - object)
  (:predicates (on ?b1 - block ?b2 - block)
               (clear ?b - block)
               (holding ?a - arm ?b - block))
  (:functions (total-cost))
  (:action pickup
    :parameters (?a - arm ?b - block)
    :precondition (and (clear ?b) (not (holding ?a ?b)))
    :effect (and (holding ?a ?b)
                 (not (clear ?b))
                 (increase (total-cost) 1))))
  (:action putdown
    :parameters (?a - arm ?b - block)
    :precondition (holding ?a ?b)
    :effect (and (not (holding ?a ?b))
                 (clear ?b)
                 (increase (total-cost) 1))))
  (:action stack
    :parameters (?b1 ?b2 - block)
    :precondition (and (clear ?b1) (not (clear ?b2)))
    :effect (and (on ?b2 ?b1)
                 (not (clear ?b1))
                 (clear ?b2)
                 (increase (total-cost) 1))))
  (:action unstack
    :parameters (?b1 ?b2 - block)
    :precondition (and (on ?b2 ?b1) (clear ?b2))
    :effect (and (not (on ?b2 ?b1))
                 (clear ?b1)
                 (not (clear ?b2))
                 (increase (total-cost) 1))))