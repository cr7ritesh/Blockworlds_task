(define (problem block-world-problem)
  (:domain block-world)
  (:objects b1 b2 b3 b4 b5 b6 - block arm1 - arm)
  (:init (on b5 b1)
         (on b4 b5)
         (on b3 b2)
         (on b1 b3)
         (on b6 b4)
         (clear b2)
         (clear b6)
         (holding arm1 nil))
  (:goal (and (on b1 b6) (on b3 b5) (on b6 b2))))