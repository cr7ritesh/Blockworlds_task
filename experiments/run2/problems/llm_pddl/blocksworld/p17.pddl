(define (problem block-world-problem)
  (:domain block-world)
  (:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 b10 b11 - block
            arm1 - arm)
  (:init (on-top-of b4 b10)
         (on-top-of b1 b11)
         (on-top-of b5 b2)
         (on-top-of b7 b8)
         (on-top-of b8 b1)
         (on-top-of b10 b7)
         (on-top-of b9 b6)
         (on-top-of b3 b4)
         (on-table b2)
         (on-table b6)
         (on-table b11)
         (clear b9)
         (clear b5)
         (clear b3)
         (empty arm1)
  (:goal (and (on-top-of b2 b7)
              (on-top-of b5 b4)
              (on-top-of b6 b10)
              (on-top-of b7 b3)
              (on-top-of b9 b5)
              (on-top-of b10 b9)
              (on-top-of b11 b1)))
  (:metric minimize (total-cost))