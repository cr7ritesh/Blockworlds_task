(define (problem blocks-problem)
  (:domain block-world)
  (:objects b1 b2 b3 b4 - block arm1 - arm)
  (:init (on b4 b2)
         (on b3 b1)
         (on b1 b4)
         (ontable b2)
         (clear b3)
         (clear b2)
         (clear b4)
         (not (holding arm1 b1))
         (not (holding arm1 b2))
         (not (holding arm1 b3))
         (not (holding arm1 b4)))
  (:goal (and (on b1 b2) (on b2 b3) (on b3 b4))))