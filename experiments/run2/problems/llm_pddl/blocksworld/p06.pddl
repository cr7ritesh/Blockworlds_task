(define (problem block-problem)
  (:domain blockworld)
  (:objects b1 b2 b3 b4 b5 - block arm1 - arm)
  (:init (on b5 b3)
         (on b4 b2)
         (on b2 b1)
         (on b3 b4)
         (clear b1)
         (clear b5)
         (not (holding arm1 b1))
         (not (holding arm1 b2))
         (not (holding arm1 b3))
         (not (holding arm1 b4))
         (not (holding arm1 b5)))
  (:goal (and (on b1 b2) (on b3 b5) (on b4 b1)))