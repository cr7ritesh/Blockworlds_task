(define (problem blocks-problem)
  (:domain block-world)
  (:objects b1 b2 b3 b4 b5 - block arm1 - arm)
  (:init (on b5 b3)
         (on b4 b2)
         (on b2 b1)
         (on b3 b4)
         (ontable b1)
         (clear b5)
         (holding arm1 nil))
  (:goal (and (on b1 b2)
              (on b3 b5)
              (on b4 b1))))