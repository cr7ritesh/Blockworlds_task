(define (problem block-world-problem)
  (:domain block-world)
  (:objects b1 b2 b3 b4 b5 b6 b7 - block arm1 - arm)
  (:init (on b4 b7)
         (on b3 b6)
         (on b7 b3)
         (clear b2)
         (clear b6)
         (clear b1)
         (clear b5)
         (clear b4)
         (not (holding arm1 b1))
         (not (holding arm1 b2))
         (not (holding arm1 b3))
         (not (holding arm1 b4))
         (not (holding arm1 b5))
         (not (holding arm1 b6))
         (not (holding arm1 b7)))
  (:goal (and (on b1 b2)
              (on b2 b6)
              (on b3 b7)
              (on b5 b3)
              (on b6 b5)
              (on b7 b4))))