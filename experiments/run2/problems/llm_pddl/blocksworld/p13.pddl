(define (problem block-world-problem)
  (:domain block-world)
  (:objects b1 b2 b3 b4 b5 b6 b7 b8 b9 - block
            arm1 - arm)
  (:init (ontable b4)
         (ontable b7)
         (clear b7)
         (clear b8)
         (empty arm1)
         (on b5 b2)
         (on b2 b3)
         (on b3 b1)
         (on b8 b6)
         (on b1 b4)
         (on b6 b9)
         (on b9 b5))
  (:goal (and (on b2 b5)
              (on b3 b1)
              (on b4 b8)
              (on b5 b3)
              (on b6 b4)
              (on b8 b7)
              (on b9 b2)))