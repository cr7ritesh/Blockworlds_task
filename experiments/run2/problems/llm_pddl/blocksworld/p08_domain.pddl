(define (domain block-world)
  (:requirements :strips :equality)
  (:types block arm)
  (:predicates (on ?b1 - block ?b2 - block)
               (clear ?b - block)
               (holding ?a - arm ?b - block))
  (:functions (total-cost))
  (:action pickup
    :parameters (?a - arm ?b - block)
    :precondition (and (clear ?b) (not (holding ?a ?_)))
    :effect (and (holding ?a ?b)
                  (not (clear ?b))
                  (decrease (total-cost) 1))
    :cost 10)
  (:action putdown
    :parameters (?a - arm ?b - block)
    :precondition (holding ?a ?b)
    :effect (and (not (holding ?a ?b))
                  (clear ?b)
                  (increase (total-cost) 1))
    :cost 10)
  (:action stack
    :parameters (?b1 ?b2 - block)
    :precondition (and (clear ?b1) (holding ?arm ?b2))
    :effect (and (on ?b2 ?b1)
                  (not (clear ?b1))
                  (not (holding ?arm ?b2))
                  (increase (total-cost) 1))
    :cost 1)
  (:action unstack
    :parameters (?b1 ?b2 - block)
    :precondition (and (on ?b2 ?b1) (clear ?b2) (not (holding ?arm ?_)))
    :effect (and (holding ?arm ?b2)
                  (clear ?b1)
                  (not (on ?b2 ?b1))
                  (decrease (total-cost) 1))
    :cost 1)
)