(define (problem block-problem)
  (:domain blockworld)
  (:objects b1 b2 b3 - block arm1 - arm)
  (:init (on b1 b3)
         (on b3 b2)
         (ontable b2)
         (clear b1)
         (clear b2)
         (clear b3)
         (not (holding arm1 b1))
         (not (holding arm1 b2))
         (not (holding arm1 b3)))
  (:goal (and (on b2 b3) (on b3 b1))))