(define (problem block-problem)
  (:domain blockworld)
  (:objects b1 b2 b3 b4 b5 b6 - block arm1 - arm table)
  (:init (on b5 b2)
         (on b2 b3)
         (on b3 b1)
         (ontable b4)
         (ontable b6)
         (ontable b1)
         (clear b6)
         (clear b5)
         (clear b4)
         (empty arm1)
