(define (domain blockworld)
  (:requirements :strips :equality)
  (:types block arm)
  (:predicates (ontable ?b - block)
               (on ?b1 - block ?b2 - block)
               (clear ?b - block)
               (emptyarm)
               (holding ?b - block))
  (:action pickup
    :parameters (?b - block)
    :precondition (and (ontable ?b) (emptyarm) (clear ?b))
    :effect (and (holding ?b) (not (ontable ?b)) (not (clear ?b)) (not (emptyarm))))
  (:action putdown
    :parameters (?b - block)
    :precondition (holding ?b)
    :effect (and (ontable ?b) (clear ?b) (emptyarm) (not (holding ?b))))
  (:action stack
    :parameters (?b1 ?b2 - block)
    :precondition (and (holding ?b1) (ontable ?b2) (clear ?b1))
    :effect (and (on ?b1 ?b2) (not (holding ?b1)) (not (clear ?b1)) (emptyarm)))
  (:action unstack
    :parameters (?b1 ?b2 - block)
    :precondition (and (on ?b1 ?b2) (emptyarm) (clear ?b2))
    :effect (and (holding ?b1) (ontable ?b1) (clear ?b2) (not (on ?b1 ?b2)) (not (emptyarm))))
)